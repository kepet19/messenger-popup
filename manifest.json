{
    "manifest_version": 2,
    "name": "facebook popup",
    "version": "1.0",
    "description": "Helper for facebook messenger to popup",
    "homepage_url": "https://github.com/kepet19",
    "browser_action": {
        "default_icon": {
            "16": "icons/page-16.png",
            "32": "icons/page-32.png"
        },
		"default_title": "Facebook popup",
		"default_popup": "popup.html"
    },
    "permissions": [
	"tabs",
    "activeTab",
	"webRequest",
	"*://*.messenger.com/"
	]
}
